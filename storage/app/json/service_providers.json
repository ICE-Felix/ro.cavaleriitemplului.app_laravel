      "placeholder": "About this provider"
    },

    "email": {
      "type": "text",
      "label": "Email",
      "placeholder": "contact@provider.ro"
    },

    "phone_no": {
      "type": "text",
      "label": "Phone number",
      "placeholder": "+40720001234"
    },

    "location": {
      "type": "location",
      "key": "location",
      "label": "Address location",
      "placeholder": "Pick on map",
      "visible": false
    },

    "address": {
      "type": "text",
      "key": "address",
      "label": "Address",
      "readonly": true,
      "visible": true
    },

    "latitude": {
      "type": "numeric",
      "label": "Latitude",
      "placeholder": "44.4268",
      "step": "0.000001"
    },

    "longitude": {
      "type": "numeric",
      "label": "Longitude",
      "placeholder": "26.1025",
      "step": "0.000001"
    },

    "business_hours": {
      "type": "schedule",
