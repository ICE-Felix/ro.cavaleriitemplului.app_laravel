    "created_at": {
      "type": "text",
      "label": "Created at",
      "readonly": true,
      "visible": false
    },
    "updated_at": {
      "type": "text",
      "label": "Updated at",
      "readonly": true,
      "visible": false
    },
    "deleted_at": {
      "type": "text",
      "label": "Deleted at",
      "readonly": true,
