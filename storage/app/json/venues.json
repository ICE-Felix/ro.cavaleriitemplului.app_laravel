{
  "name": {
    "singular": "venue",
    "plural": "venues",
    "label_singular": "Venue",
    "label_plural": "Venues"
  },
  "debug": ["UPDATE"],
  "query_params": {
    "include_galleries": true
  },
  "schema": {
    "id": {
      "label": "#",
      "type": "int",
      "readonly": true,
      "visible": false
    },
    "image_url": {
      "key": "image_id",
      "type": "image",
      "label": "Featured image",
      "upload_key": "image_base64"
    },
    "name": {
      "type": "text",
      "label": "Name",
      "placeholder": "Name",
      "required": true
    },
    "contact_name": {
      "type": "select",
      "label": "Contact",
      "key": "contact_id",
      "data": {
        "type": "class",
        "source": [
          "App\\Services\\Supabase\\SupabaseService",
          "read_edge",
          "contacts"
        ],
        "value": "id",
        "name": "{first_name} {last_name}"
      },
      "required": true
    },
    "categories": {
      "type": "checkbox",
      "label": "Categories",
      "key": "venue_category_id",
      "value": [],
      "visible": false,
      "required": true,
      "data": {
        "type": "class",
        "source": [
          "App\\Services\\Supabase\\SupabaseService",
          "read_edge",
          "venue_categories"
        ],
        "value": "id",
        "name": "name"
      }
    },
    "venue_category_titles": {
      "type": "text",
      "label": "Categories",
      "readonly": true,
      "visible": true
    },
    "attribute_ids": {
      "type": "checkbox",
      "label": "Addressability",
      "value": [],
      "key": "attribute_ids",
      "visible": false,
      "required": true,
      "data": {
        "type": "class",
        "source": [
          "App\\Services\\Supabase\\SupabaseService",
          "read_edge",
          "venue_general_attributes"
        ],
        "value": "id",
        "name": "{type} - {value}"
      }
    },
    "card_details": {
      "type": "text",
      "label": "Card details",
      "placeholder": "Intrare 65 RON",
      "required": false
    },
    "attribute_names": {
      "type": "text",
      "label": "Attributes",
      "readonly": true,
      "visible": true
    },
    "description": {
      "type": "trix",
      "label": "Description",
      "placeholder": "Description"
    },
    "phone_no": {
      "type": "text",
      "label": "Phone number",
      "placeholder": "+40720001222"
    },
    "email": {
      "type": "text",
      "label": "Email",
      "placeholder": "contact@venue.ro"
    },
    "location": {
      "type": "location",
      "key": "location",
      "label": "Address location",
      "placeholder": "Address location",
      "visible": false
    },
    "address": {
      "type": "text",
      "key": "address",
      "label": "Address",
      "readonly": true,
      "visible": true
    },
    "is_active": {
      "type": "switch",
      "label": "Venue Status",
      "key": "is_active",
      "value": true,
      "on_label": "Active",
      "off_label": "Inactive"
    },
    "order_display": {
      "type": "text",
      "label": "Order Display",
      "value": "0",
      "placeholder": "0",
      "visible": true
    },
    "business_hours": {
      "type": "schedule",
      "label": "Business Hours",
      "key": "business_hours",
      "value": null,
      "required": false
    },
    "gallery": {
      "type": "gallery",
      "label": "Venue Gallery",
      "key": "gallery",
      "value": null,
      "required": false,
      "visible": false,
      "min_images": 0,
      "max_images": 5
    }
  },
  "GET": "edge",
  "INSERT": "edge",
  "DELETE": "edge",
  "UPDATE": "edge"
}
